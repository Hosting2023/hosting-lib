{
  "compilerOptions": {
    "lib": ["es6", "dom", "es2017"],
    "target": "es5",
    "module": "commonjs",
    "explainFiles": true,
    "strict": true,
    "outDir": "./dist",
    "declaration": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": ["./**/*.ts"], 
  "exclude": ["node_modules"],
}